                                "properties": {
                                    "email": {
                                        "type": "email"
                                    },
                                    "password": {
                                        "type": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Succesfully Login"
                    },
                    "201": {
                        "description": "Succesfully Login"
                    },
                    "419": {
                        "description": "Unauthorhized"
                    }
                }
            }
        },
