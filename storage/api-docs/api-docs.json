                "operationId": "2dd578ff7aba2721293dac66833b27d1",
                "requestBody": {
                    "description": "- Login to add products",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
